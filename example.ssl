function bubbleSort(inputArr:integer):integer
{
    var len:integer = inputArr.length;
	var i:integer;
	var j:integer;
    for (i = 0; i < len; i++) {
        for (j = 0; j < len; j++) {
            if (inputArr[j] > inputArr[j + 1]) {
                var tmp = inputArr[j];
                inputArr[j] = inputArr[j + 1];
                inputArr[j + 1] = tmp;
            }
        }
    }
}